pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--paul smith
--lost vickings pico-8 idea
--copyrighted by blizzard 
--this is just a fan demo

function _init()
frame=0
init_player()
palt(14,true)
palt(0,false)
beams={}
makebeam(17,3,3)
doors={}
makedoor(22,7,true)
bullets={}
makebullet(26,6,-1)
end

function makebullet(bx,by,bdir)
bu={}

bu.x=bx*8
bu.y=(by*8)+2
bu.dir=bdir
if(bu.dir<0) then
bu.flip=false
else
bu.flip=true
end
bu.exp=false
bu.a=0
add(bullets,bu)
return bu
end

function init_player()
p={}
p.x=64
p.y=48
p.a=0
p.w=false
p.d=false

return p
end

function makedoor(dx,dy,dro)
d={}
d.x=dx 
d.y=dy 
d.o=dro
add(doors,d)
return
end



function makebeam(bx,by,bl)
b={}
b.x=bx*8
b.y=by*8
b.l=bl
b.f=0
add(beams,b)
return b
end

function _update()
frame+=1


if (frame==30) then frame=0 end
p.w=false --not walking

if (btn(0)) then
 p.x-=2
 p.w=true
 p.d=true
end

if (btn(1)) then
	p.x+=2
	p.w=true
	p.d=false
end

if (btn(4)) then
	d.o=false
end

if(p.w==true and frame%4==0) then
p.a+=1
if(p.a>1) then p.a=0 end
end
foreach(beams,updatebeams)
end

function updatebeams(b)
if (frame%4==0) then
b.f+=1
if(b.f==2) then b.f=0 end
end

foreach(bullets,updatebullet)

end

function updatebullet(bu)
if(bu.exp==true) then
	if(frame%3==0) then
		bu.a+=2
	end
	if(bu.a>4) then 
		del(bullets,bu)
		makebullet(26,6,-1)
	end
else
		bu.x+=bu.dir
	if(mget(bu.x/8,bu.y/8)==118) then
		bu.exp=true
	end 
end
end


function _draw()
cls()

map((p.x/8),0,0-(p.x%8),0,18,12)

if (p.w==false) then
spr(4,64,p.y,2,2,p.d,false)
else
spr(6+p.a*2,64,p.y,2,2,p.d,false)
end
foreach (beams,drawbeams)
draw_bottom()
foreach(doors,drawdoor)
foreach(bullets,drawbullet)
--rect(0,0,127,127)
end





function drawbeams(b)
 for i=1,b.l do	
		spr(74+b.f,b.x+(i*8)-p.x,b.y)
 end
end

function drawdoor(d)
if(d.o==true) then
mset(d.x,d.y-1,118)
mset(d.x,d.y,119)
else
mset(d.x,d.y-1,0)
mset(d.x,d.y,0)
end
--spr(119,d.x-p.x,d.y)
--spr(118,d.x-p.x,d.y-8)

end

function drawbullet(bu)

		spr(120+bu.a,bu.x-p.x,bu.y)

end

function draw_bottom()
map(0,60,0,96,17,4)
end
